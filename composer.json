{
	"name": "jumilla/laravel-addomnipot",
	"type": "framework-extension",
	"description": "Addon system for Laravel.",
	"license": "MIT",
	"keywords": ["Laravel", "Laravel 5", "module", "addon", "sub application", "facade"],
	"homepage": "http://jumilla.me",
	"authors": [
		{
			"name": "Fumio Furukawa",
			"email": "fumio@jumilla.me"
		}
	],
	"require": {
		"php": ">=5.5.9",
        "illuminate/console": "5.1.*",
        "illuminate/container": "5.1.*",
        "illuminate/contracts": "5.1.*",
        "illuminate/filesystem": "5.1.*",
        "illuminate/support": "5.1.*",
        "jumilla/laravel-source-generator": "^1.0"
	},
    "require-dev": {
        "illuminate/config": "5.1.*",
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9"
    },
	"autoload": {
		"psr-4": {
			"Jumilla\\Addomnipot\\Laravel\\": "sources/"
		},
		"files": [
			"sources/helpers.php"
		]
	},
	"minimum-stability": "stable",
	"prefer-stable": true
}
